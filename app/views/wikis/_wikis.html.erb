<table class="responsive">
<thead>
  <tr>
      <th width="200">Wikiname</th>
      <th width="250">Description</th>
      <th width="130">Latest Modification</th>
      <th width="100">Contribute</th>
      
    </tr>
</thead>
<tbody>
<% wikis.each do |wiki| %>
<% if can? :read, wiki %>
<tr>
  <td><%= link_to wiki.wikiname, wiki %></td>
  <td><%= wiki.description %></td>
  <td><%= time_ago_in_words(wiki.updated_at) %></td>
  <td><%= link_to "Edit", edit_wiki_path(wiki) %></td>
</tr>
<% end %>
<% end %>
</tbody>
</table>